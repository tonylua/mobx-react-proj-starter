  "description": "mobx+react+react_router_4+webpack2+eslint+mock_api",
  "main": "dist/index.html",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "mock": "nodemon ./mock.server.js",
    "build": "cross-env NODE_ENV=production PLATFORM=web webpack --color",
    "preview": "nodemon ./preview.server.js",
    "eslint": "eslint src/ --ext .jsx,.js",
    "eslint-echo": "echo \"start eslint...\" && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "pre-commit": [
    "eslint-echo",
    "eslint"
  ],
